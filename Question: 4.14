#include <stdio.h>

int main(){
    
    int i, j, factorial;

    printf("   Number    Factorial \n");

    for ( i = 1; i <= 5; i++)
    {
    factorial = 1;
      
      for (j = 1; j <= i; j++)
      {
        factorial *= j;
      }
      printf("%6d %10d \n", i, factorial);
    }
    
    return 0;
}
