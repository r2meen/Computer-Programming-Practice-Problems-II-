#include <stdio.h>

int main(){
    
    int n, i;
    long sum = 0, squares = 0, cubes = 0;

    printf("Enter an integer: \n");
    scanf("%d", &n);

    for ( i = 1; i <= n; i++)
    {
        sum += i;
        squares += i*i;
        cubes += i*i*i;
    }
    
    printf("Sum of numbers from 1 to %d: %ld \n", n, sum);
     printf("Sum of squares from 1 to %d: %ld \n", n, squares);
      printf("Sum of cubes from 1 to %d: %ld \n", n, cubes);

    return 0;
}
