#include <stdio.h>

int main(){
    
    int num, original, remainder, result;

    printf("Three-digit Armstrong numbers are: \n");

    for(num = 100; num <= 999; num++){
        original = num;
        result = 0;
        
        while (original != 0) {
            remainder  = original % 10;
            result += remainder*remainder*remainder;
            original /= 10;
        }
        
        if (result == num){
            printf("%d \n",num);
         }  
    }
    return 0;
}
