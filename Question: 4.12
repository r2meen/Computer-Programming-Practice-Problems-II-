#include <stdio.h>

int main(){
    
    int i, j, prime;

    printf("Prime numbers from 1 to 100 is: \n");

    for(i = 2; i <= 100; i++){
        prime = 1;

     for(j = 2; j * j <= i; j++){
        if(i % j == 0){
            prime = 0;
            break;
        }
     }

     if (prime){
     printf("%d ",i);
    }
}
    printf(" \n ");

    return 0;
}
